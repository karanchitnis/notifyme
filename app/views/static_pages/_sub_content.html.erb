<% if @feed and @feed.any? %>
	<% for item in @user_subscription.feeds %>
    <% if item.is_read %>
    		<div class="read_feed_item" id="<%= item.url %>">
    <% else %>
        <div class="unread_feed_item" id="<%= item.url %>">
    <% end %>

          <!--<li>TITLE: <a href="<%#= item.url %>" title="<%#= item.title%>"><%#= item.title%></a></li>
          <li>AUTHOR: <%#= item.author %></li>
          <li>SUMMARY: <%#= item.summary %></li>-->

    <li>URL: <a href="<%= item.url %>" title="Click to read the article"><%= item.url%></a></li>
    <li>PUBLISHED: <%= item.published %></li>
    <br/><br/>
    <% end %>
    </div>
<% end %>
